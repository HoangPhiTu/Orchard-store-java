NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_ACCESS_TOKEN_KEY=orchard_admin_token

# JWT Secret for token verification in middleware (optional but recommended for security)
# Should match the JWT secret used in the backend Spring Boot application
# If not provided, middleware will decode tokens without verification (less secure but faster)
# For production, always provide this value
JWT_SECRET=your-jwt-secret-key-here

